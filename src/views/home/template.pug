v-container.home
  v-row(justify="center", align-content="center", style="height: calc(100vh - 200px);")
    v-col(cols=12)
      img(alt="Vue logo" src="../../assets/logo.png")
    v-col(cols=12)
      .display-1 {{ state.text }}
    v-col(cols=12)
      .display-1 {{ state.count }}
      v-btn.mx-1(color="primary" x-small fab @click="addCount()")
        v-icon add
      v-btn.mx-1(color="error" x-small fab @click="minusCount()")
        v-icon mdi-minus

